rule __trimjaTouch
  command = cmd /c type nul > $out_backslashes
rule copy
  command = ninja --version $in -> $out
build a.txt: copy b.txt d.txt f.txt | $builddir/__trimja__/1 $builddir/__trimja__/2
build b.txt: copy c.txt
build c.txt: copy d.txt
build e.txt: copy d.txt
build f.txt: copy g.txt
build g.txt: copy h.txt
build h.txt: copy i.txt
build $builddir/__trimja__/1: __trimjaTouch | b.txt
build $builddir/__trimja__/2: __trimjaTouch | $builddir/__trimja__/3
build $builddir/__trimja__/3: __trimjaTouch | e.txt
